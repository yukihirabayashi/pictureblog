<h1><%#= image_tag current_user.icon %> <%= current_user.email %>のお気に入りブログ一覧</h1>

    <p>

  <div class="row">

    <% @favorites_pictures.each do |favorete| %>


      <div class="col-xs-12 col-md-6 blog-panel">
        <table border=0>

          <tr>
            <td> <%= image_tag(favorete.image) %></td>
          </tr>
          <tr>
            <td><%= favorete.user.email %></td>
          </tr> 
          <tr>
            <td><%= favorete.content %></td>
          </tr> 

        </table>
        <br>
      </div>
    <% end %>
  </div>
